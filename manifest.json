{
  "manifest_version": 3,
  "name": "Submitty Tweaks",
  "version": "1.0",
  "description": "Prefetch Submitty links for faster navigation.",
  "content_scripts": [
    {
      "matches": ["https://submitty.cs.rpi.edu/*"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ],
  "icons": {
    "48": "icon.png"
  }
}

